<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pm.dao.datasource.Point" table="t_point" schema="pm">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="pointValue">
            <column name="point_value" sql-type="int(11)"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="ptId">
            <column name="pt_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>