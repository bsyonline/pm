<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pm.dao.datasource.Goods" table="t_goods" schema="pm">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="goodsId">
            <column name="goods_id" sql-type="varchar(255)"/>
        </property>
        <property name="goodsName">
            <column name="goods_name" sql-type="varchar(255)"/>
        </property>
        <property name="goodsPrice">
            <column name="goods_price" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>